<%= form_with model: @papular_palace, html: { data: {turbo: false } } do |form| %>
  <div class="form-outline mb-4">
		<%= form.label :name, class:"form-label" %>
		<%= form.text_field :name, class:"form-control" %>
		<% @papular_palace.errors.full_messages_for(:name).each do |message| %>
		<p class="text-danger"><%= message  %></p>
		<% end %>
	</div>

	<div class="form-outline mb-4">
		<%= form.label :state, class:"form-label" %>
		<%= form.text_field :state, class:"form-control" %>
		<% @papular_palace.errors.full_messages_for(:state).each do |message| %>
		<p class="text-danger"><%= message  %></p>
		<% end %>
	</div>
	
	<div class="form-outline mb-4">
		<%= form.label :city, class:"form-label" %>
		<%= form.text_field :city, class:"form-control" %>
		<% @papular_palace.errors.full_messages_for(:city).each do |message| %>
		<p class="text-danger"><%= message  %></p>
		<% end %>
	</div>

	<div class="form-outline mb-4">
		<%= form.label :About, class:"form-label" %>
		<%= form.text_area :about, class:"form-control" %>
		<% @papular_palace.errors.full_messages_for(:about).each do |message| %>
		<p class="text-danger"><%= message  %></p>
		<% end %>
	</div>

	<div class="form-outline mb-4">
		<%= form.label :palace_images, class:"form-label" %>
		<%= form.file_field :palace_images, multiple: true, class:"form-control" %>
		<% @papular_palace.errors.full_messages_for(:palace_images).each do |message| %>
		<p class="text-danger"><%= message  %></p>
		<% end %>
	</div>

	<%= form.submit class:"btn btn btn-outline-primary m-2" %>




<% end %>