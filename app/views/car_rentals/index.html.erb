<div class="col-lg-12">
  <div class="container">
    <p><%= link_to "Car Rental", new_car_rental_path, class:"btn btn-outline-warning"%></p>
    <table class="table table-hover">
        <h4 class="text-center">Car Rental</h4>
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">Car Number</th>
            <th scope="col">Pick up location</th>
            <th scope="col"> Drop off location</th>
            <th scope="col"> Contact Number</th>
            <th scope="col"> Action</th>
          </tr>
        </thead>
        <tbody>
          <% @car_rentals.each do |car| %>
           <tr>
            <th scope="row"><%= car.id %></th>
            <td><%= car.car_number%></td>
            <td><%= car.pick_up_location%></td>
            <td><%= car.drop_off_location%></td>
            <td><%= car.contact_number%></td>
            <td><%= link_to "More Details", car , class:"btn btn-primary"%></td>
           </tr>
          <%end %>
        </tbody>
    </table>
  </div>
</div>
