<div class="col-lg-12">
	<div class="container">
		<%= link_to "Add ", new_tourist_point_path, class:"btn btn-primary"%>
		<div class="col-lg-10">
			<div class="container">
				<table class="table">
					<thead>
						<tr>
							<th scope="col">#</th>
							<th scope="col">Place name</th>
							<th scope="col">Location name</th>
							<th scope="col">Action</th>
						</tr>
					</thead>
					<tbody>
						<% @tourist_points.each do |tourist_point| %>
						<tr>
							<td><%= tourist_point.id%></td>
							<td><%= tourist_point.place_name%></td>
							<td><%= tourist_point.location_name %></td>
							<td><%= link_to "show", tourist_point, class:"btn btn-warning" %></td>
						</tr>
						<% end %>
					</tbody>
				</table>
			</div>
		</div>

	</div>
</div>

